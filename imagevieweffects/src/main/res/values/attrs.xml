<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ImageViewEffects">
        <attr name="mTitle" format="string"/>
        <attr name="mImage" format="reference"/>
        <attr name="mEffect" format="integer">
            <enum name="none" value="0" />
            <enum name="fade" value="1" />
            <enum name="slide" value="2" />
            <enum name="move" value="3" />
            <enum name="uncover" value="4" />
            <enum name="corner" value="5" />
            <enum name="ribbon" value="6" />
        </attr>
    </declare-styleable>

</resources>